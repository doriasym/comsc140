def deleteOne(numbers, value):
    while True:
        try:
            numbers.remove(value)
        except ValueError:
            break


def main():
    numbers = [5, 20, 30, 30, 35]
    print(f'Original list value: {numbers}')
    deleteOne(numbers, 30)
    print(f'After deleting 30 {numbers}')


if __name__ == '__main__':
    main()